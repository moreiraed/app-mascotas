{
  "expo": {
    "name": "app-mascotas",
    "slug": "app-mascotas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/cat-dog-logo-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/cat-dog-logo-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "expo": {
            "splash": {
              "image": "./assets/cat-dog-logo-splash-icon.png",
              "resizeMode": "contain",
              "backgroundColor": "#ffffff"
            }
          }
        }
    
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
